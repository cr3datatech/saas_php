{
    "name": "saas-php/business-idea-generator",
    "description": "PHP version of Business Idea Generator using OpenAI API",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "openai-php/client": "^0.19",
        "symfony/http-client": "^8.0",
        "nyholm/psr7": "^1.8",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
